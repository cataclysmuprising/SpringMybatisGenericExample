<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
"http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	<definition name="user_home" extends="baseLayout">
		<put-attribute name="content" value="/WEB-INF/views/user/home.jsp" />
		<put-list-attribute name="pageScripts">
			<add-attribute value="/js/jquery/datatables.min.js" />
			<add-attribute value="/js/site/user/home.js" />
		</put-list-attribute>			
		<put-list-attribute name="pageStyles">
			<add-attribute value="/css/jquery/datatables.min.css" />
			<add-attribute value="/css/site/user/home.css" />
		</put-list-attribute>
	</definition>	
	
	<definition name="user_dataForm" extends="baseLayout">
		<put-attribute name="content" value="/WEB-INF/views/user/dataInputForm.jsp" />
		<put-list-attribute name="pageScripts">
			<add-attribute value="/js/common/moment.min.js" />
			<add-attribute value="/js/bootstrap/bootstrap-datetimepicker.min.js" />
			<add-attribute value="/js/common/md5.js" />
			<add-attribute value="/js/site/user/dataInputForm.js" />
			<add-attribute value="/js/jquery/cropper.min.js" />
		</put-list-attribute>			
		<put-list-attribute name="pageStyles">
			<add-attribute value="/css/bootstrap/bootstrap-datetimepicker.min.css" />
			<add-attribute value="/css/site/user/dataInputForm.css" />
			<add-attribute value="/css/jquery/cropper.min.css" />
		</put-list-attribute>
	</definition>	
</tiles-definitions>